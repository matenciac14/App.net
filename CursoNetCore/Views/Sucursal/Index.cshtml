@using CursoNetCore.Models;
@model List<SucursalCLS>
@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<table class="table table-bordered table-hover">
    <thead>
        <tr>
            <td width="10%">@Html.LabelFor(p => Model[0].iidsucursal)</td>
            <td width="20%">@Html.LabelFor(p => Model[0].nombre)</td>
            <td width="20%">@Html.LabelFor(p => Model[0].telefono)</td>
            <td width="60%">@Html.LabelFor(p => Model[0].email)</td>
        </tr>

    </thead>
    <tbody>
        @foreach (var item in Model)
        {
        <tr>
            <td>@item.iidsucursal</td>
            <td>@item.nombre</td>
            <td>@item.telefono</td>
            <td>@item.email</td>
        </tr>

        }

    </tbody>
</table>